{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/api/notes.php",
      "destination": "/api/notes.php"
    },
    {
      "source": "/api/upload.php",
      "destination": "/api/upload.php"
    }
  ]
}
